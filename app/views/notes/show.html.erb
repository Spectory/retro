<div class='row main-page-with-margin'>
  <div class="note-in-column panel <%= @note.positive? ? "panel-success" : "panel-danger" %>">
    <div class="panel-heading"><%= @note.title %></div>
    <div class="panel-body"><%= @note.body %></div>
    <div class="panel-footer">Created By: <%= @note.create_by %></div>
  </div>
</div>
