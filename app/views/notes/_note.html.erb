<tr>
  <td><%= note.title %></td>
  <td><%= note.body %></td>
  <td><%= note.create_by %></td>
  <td>
    <%= link_to edit_note_path(note) do %>
      <span class="glyphicon glyphicon-edit"></span>
    <% end %>
  </td>
  <td>
    <%= link_to "Delete", note, :method => :delete, :confirm => "Are you sure ?"%>
    <span class="glyphicon glyphicon-remove"></span></td>
</tr>
