<!DOCTYPE html>
<html>
<head>
  <title>Retro</title>
  <%= stylesheet_link_tag    "application", media: "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% if user_signed_in? %>
    <div class='main-wrapper logged-in'>
      <%= render 'layouts/header' %>
      <%= yield %>
    </div>
  <% else %>
    <div class='main-wrapper'>
      <%= yield %>
    </div>
  <% end %>
  <%= render 'layouts/footer' %>
  </div>
</body>
</html>
